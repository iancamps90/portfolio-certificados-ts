{
  "version": 2,
  "builds": [
    {
      "src": "package.json",
      "use": "@vercel/static-build",
      "config": {
        "distDir": "build"
      }
    }
  ],
  "routes": [
    {
      "src": "/certificados/(.*\\.pdf)",
      "headers": {
        "Content-Type": "application/pdf",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/certificados/$1"
    },
    {
      "src": "/static/css/(.*)",
      "headers": {
        "Content-Type": "text/css",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/css/$1"
    },
    {
      "src": "/static/js/(.*)",
      "headers": {
        "Content-Type": "application/javascript",
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/js/$1"
    },
    {
      "src": "/static/media/(.*)",
      "headers": {
        "cache-control": "public, max-age=31536000, immutable"
      },
      "dest": "/static/media/$1"
    },
    {
      "src": "/(favicon\\.ico|logo192\\.png|logo512\\.png|manifest\\.json|robots\\.txt)",
      "dest": "/$1"
    },
    {
      "src": "/(.*)",
      "dest": "/index.html"
    }
  ]
}
